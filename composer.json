{
    "name": "lmn/composer-update-manager",
    "license": "MIT",
    "description": "Library for installing composer updates via code",
    "keywords": ["php", "composer update", "package version", "package manager", "package version", "package", "version", "manager", "composer"],
    "authors": [
        {
            "name": "Peter Gasparik",
            "email": "gasparik.pe@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
        "php": "5.6.* || 7.*",
        "composer/composer": "1.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Lmn\\ComposerUpdateManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lmn\\ComposerUpdateManager\\Test\\": "test/"
        }
    }
}
